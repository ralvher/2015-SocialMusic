<% if logged_in? %>
	<div class="row">
		<aside class="col-md-4 col-xs-12 col-lg-4 col-sm-12">
			<section class="user_info">
				<%= render 'shared/user_info' %>
			</section>
		      <section class="stats">
		        <%= render 'shared/stat' %>
		      </section>
			<section class="micropost_form">
				<%= render 'shared/micropost_form' %>
			</section>
		</aside>
		<div class="time col-md-8 col-xs-12 col-lg-8 col-sm-12">
			<h3>Timeline</h3>
			<%= render 'shared/feed' %>
		</div>
	</div>
<% else %>
	<%= render 'sessions/registro_form' %>
<% end %>