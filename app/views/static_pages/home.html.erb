<% if logged_in? %>
	<div class=" myrow row col-md-12 col-xs-12 col-lg-12 col-sm-12">
		<aside class="myp col-md-3 col-xs-12 col-lg-3 col-sm-12">
			<section class="user_info">
				<%= render 'shared/user_info' %>
			</section>
		     <section class="stats">
		        <%= render 'shared/stat' %>
		     </section>	

		     <section class="micropost_form">
				<%= render 'shared/micropost_form' %>
			</section>		
		</aside>

		
		<div class="time col-md-6 col-xs-offset-0 col-sm-offset-0 col-xs-12 col-lg-6 col-sm-12">
			<%= render 'shared/feed' %>
		</div>

		<div class="exitos col-md-3 col-xs-12 col-lg-3 col-sm-12">
			<%= render 'shared/descubrir' %>
		</div>
	</div>
<% else %>
	<%= render 'sessions/registro_form', :@notfollow => @notfollow %>
	
<% end %>